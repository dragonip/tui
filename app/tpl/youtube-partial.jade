- if (things.length > 0)
	- for (var i = startIndex; i < things.length; i++ )
		div(class='item '+ (alterClass ? 'larger':'regular'), data-sequence=i)
			div.imgcont
				div.video-bar
					div.rating_bar
						div(style="width:" + things[i].starRating +"%")
				img(src=things[i].thumbnail.sqDefault)
			span.title!= things[i].title
