div.tui-multiscreen.callHistory
	div.callHistoryItem.nobg
		div.caller!= header.caller
		div.time!= header.time
		div.duration!= header.duration
		div.type!= header.type
	div.callHistoryResults
		- if (typeof things === 'object')
			- for (var i = 0; i < things.length; i++ )
				div.callHistoryItem
					div.caller!= things[i].callernum
					div.time!= things[i].starttime
					div.duration!= things[i].duration
					div.type
						- if(things[i].type === "received")
							img(src="app/imgs/received.svg")
						- else if(things[i].type === "missed")
							img(src="app/imgs/missed.svg")
						- else if(things[i].type === "dialed")
							img(src="app/imgs/dialed.svg")
		- else
			div.emptyScreen
